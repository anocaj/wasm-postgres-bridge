# Environment variables for WASM-PostgreSQL learning project
# Copy this file to .env and update the values as needed

# Database configuration
DATABASE_URL=postgresql://postgres:password@localhost:5432/wasm_learning
DB_HOST=localhost
DB_PORT=5432
DB_NAME=wasm_learning
DB_USER=postgres
DB_PASSWORD=password

# WebSocket server configuration
WEBSOCKET_PORT=8080

# Development settings
NODE_ENV=development
LOG_LEVEL=info